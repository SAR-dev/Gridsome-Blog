m \"../../static/corner-left-down.svg\";\nexport default {\n  metaInfo: {\n    title: \"Home\"\n  },\n  components: {\n    Smile: Smile,\n    Below1: Below1,\n    Below2: Below2\n  }\n};",{"version":3,"sources":["Index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8NA,OAAA,KAAA;AACA,OAAA,MAAA;AACA,OAAA,MAAA;AAEA,eAAA;AACA,EAAA,QAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA,GADA;AAIA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA,KADA;AAEA,IAAA,MAAA,EAAA,MAFA;AAGA,IAAA,MAAA,EAAA;AAHA;AAJA,CAAA","sourcesContent":["<template>\n  <Layout>\n    <div class=\"hero container mx-auto flex flex-col sm:flex-row justify-between pt-16\">\n      <div class=\"sm:w-2/5 flex flex-col items-center sm:items-start text-center sm:text-left\">\n        <h1\n          class=\"uppercase text-5xl text-blue-900 font-bold leading-none tracking-wide mb-2\"\n        >Comics Ghor</h1>\n        <h2\n          class=\"uppercase text-xl text-orange-500 text-secondary tracking-widest mb-6\"\n        >A CANVAS TO PAINT YOUR MIND OUT</h2>\n        <p\n          class=\"text-gray-600 text-lg mb-6\"\n        >Welcome to Comics Ghor, a Bangladeshi website. The purpose of Comics Ghor is to create a free platform for all the Bangladeshi people, of all age, who desire to draw comics or write novels, stories or more.</p>\n        <p\n          class=\"text-gray-600 text-lg mb-6\"\n        >The idea of creating Comics Ghor was a dream for quite long. We want nothing but to unleash your latent talents without hesitation. Comics Ghor is created with that purpose, as the home of all creative people.</p>\n      </div>\n\n      <div class=\"mb-10 sm:mb-0 mt-8 sm:mt-0 sm:w-3/5 sm:px-12\">\n        <carousel :autoplay=\"true\" :perPage=\"1\" :autoplayTimeout=\"5000\">\n          <slide>\n            <g-image src=\"../../static/carousel (1).svg\" class=\"object-cover\" />\n          </slide>\n          <slide>\n            <g-image src=\"../../static/carousel (2).svg\" class=\"object-cover\" />\n          </slide>\n          <slide>\n            <g-image src=\"../../static/carousel (3).svg\" class=\"object-cover\" />\n          </slide>\n          <slide>\n            <g-image src=\"../../static/carousel (4).svg\" class=\"object-cover\" />\n          </slide>\n        </carousel>\n      </div>\n    </div>\n\n    <div class=\"overflow-x-hidden mt-8\">\n      <div\n        class=\"projects container-inner mx-auto text-xl border-t border-gray-500 border-b py-16 relative\"\n      >\n        <h2\n          class=\"font-black mb-6 text-2xl leading-none tracking-wide\"\n          id=\"projects\"\n          style=\"color: #D53F8C; font-weight: 800\"\n        >SOCIAL NETWORKING WEBSITE COMING SOON</h2>\n        <div class=\"mb-20\">\n          <g-image src=\"../../static/party-popper.png\" width=\"35\" class=\"float-left\" />\n          <g-image src=\"../../static/party-popper.png\" width=\"35\" class=\"float-left\" />\n          <g-image src=\"../../static/party-popper.png\" width=\"35\" class=\"float-left\" />\n        </div>\n        <div class=\"absolute right-0\" style=\"top: 50px; transform: translate(100%) rotate(180deg)\">\n          <svg width=\"170px\" height=\"170px\">\n            <use xlink:href=\"#dots-triangle\" />\n          </svg>\n        </div>\n\n        <ul class=\"text-lg sm:text-xl\">\n          <li class=\"checkmark mb-6\">\n            <div>Have your own personalized account</div>\n          </li>\n          <li class=\"checkmark mb-6\">\n            <div>Able to create Discussion posts</div>\n          </li>\n          <li class=\"checkmark mb-6\">\n            <div>Create your own series</div>\n          </li>\n          <li class=\"checkmark mb-6\">\n            <div>Upload your own comics and stories</div>\n          </li>\n          <li class=\"checkmark mb-6\">\n            <div>Promote your pop cultural business through us</div>\n          </li>\n          <li class=\"checkmark mb-6\">\n            <div>Share your talents through us to the world</div>\n          </li>\n          <li class=\"checkmark mb-6\">\n            <div>Learn more of pop culture through us</div>\n          </li>\n        </ul>\n      </div>\n      <!-- end projects -->\n    </div>\n\n    <div class=\"container-inner text-center mx-auto py-10 border-b border-gray-500\">\n      <h3 class=\"font-bold text-3xl uppercase leading-none tracking-wide\" style=\"color: #B83280\">\n        Click below\n        <div class=\"float-left text-5xl\">\n          <Below2 />\n        </div>\n        <div class=\"float-right text-5xl\">\n          <Below1 />\n        </div>\n      </h3>\n      <a\n        href=\"mailto:comicsghor@gmail.com?cc=sayedarrafi@iut-dhaka.edu&bcc=ashraful.bilad01@gmail.com&subject=We want to SPONSOR your website!&\"\n        class=\"text-white hover:text-gray-400\"\n      >\n        <g-image src=\"../../static/sponsor.png\" alt=\"Sponsor Us\" />\n      </a>\n    </div>\n\n    <div id=\"container-inner\" class=\"w-4/5 mx-auto py-10 border-b border-gray-500\">\n      <h2\n        class=\"uppercase text-5xl text-blue-900 font-bold leading-none tracking-widest mb-6 text-center\"\n      >Meet the founders</h2>\n      <div class=\"flex flex-col sm:flex-row justify-center\">\n        <div class=\"sm:w-1/4 p-2 mx-4\">\n          <div class=\"bg-white px-6 py-8 rounded-lg shadow-lg text-center\">\n            <div class=\"mb-3\">\n              <g-image\n                src=\"https://i.imgur.com/lLyJqBI.jpg\"\n                class=\"w-48 mx-auto rounded-full h-48 object-cover\"\n                alt=\"Sayed Rafi\"\n              />\n            </div>\n            <h2 class=\"text-xl font-medium text-gray-700 font-bold\">Sayed Rafi</h2>\n            <span class=\"text-blue-500 block mb-5\">Founder and Programmer</span>\n          </div>\n        </div>\n\n        <div class=\"sm:w-1/4 p-2 mx-4\">\n          <div class=\"bg-white px-6 py-8 rounded-lg shadow-lg text-center\">\n            <div class=\"mb-3\">\n              <g-image\n                src=\"https://i.imgur.com/FCYsae6l.png\"\n                class=\"w-48 mx-auto rounded-full h-48 object-cover\"\n                alt=\"Ashraful Ribat\"\n              />\n            </div>\n            <h2 class=\"text-xl font-medium text-gray-700 font-bold\">Ashraful Ribat</h2>\n            <span class=\"text-blue-500 block mb-5\">Founder and Owner</span>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"container inline\">\n      <h2\n        class=\"uppercase text-5xl text-blue-900 font-bold leading-none tracking-widest mb-6 text-center py-8\"\n      >Contact Us</h2>\n      <form class=\"w-full max-w-lg mx-auto px-5\" action=\"https://formspree.io/meqkdyzo\" method=\"POST\">\n        <div class=\"flex flex-wrap -mx-3 mb-6\">\n          <div class=\"w-full md:w-1/2 px-3 mb-6 md:mb-0\">\n            <label\n              class=\"block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2\"\n              for=\"grid-first-name\"\n            >First Name</label>\n            <input\n              class=\"appearance-none block w-full bg-gray-200 text-gray-700 border border-red-500 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white\"\n              id=\"grid-first-name\"\n              type=\"text\"\n              placeholder=\"Jane\"\n              name=\"First Name\"\n              required\n            />\n          </div>\n          <div class=\"w-full md:w-1/2 px-3\">\n            <label\n              class=\"block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2\"\n              for=\"grid-last-name\"\n            >Last Name</label>\n            <input\n              class=\"appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500\"\n              id=\"grid-last-name\"\n              type=\"text\"\n              placeholder=\"Doe\"\n              name=\"Last Name\"\n              required\n            />\n          </div>\n        </div>\n        <div class=\"flex flex-wrap -mx-3 mb-6\">\n          <div class=\"w-full px-3\">\n            <label\n              class=\"block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2\"\n              for=\"grid-password\"\n            >E-mail</label>\n            <input\n              class=\"appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white focus:border-gray-500\"\n              id=\"email\"\n              type=\"email\"\n              placeholder=\"john-doe@gmail.com\"\n              name=\"Email\"\n              required\n            />\n          </div>\n        </div>\n        <div class=\"flex flex-wrap -mx-3 mb-6\">\n          <div class=\"w-full px-3\">\n            <label\n              class=\"block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2\"\n              for=\"grid-password\"\n            >Message</label>\n            <textarea\n              class=\"no-resize appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 h-48 resize-none\"\n              id=\"message\"\n              name=\"Message\"\n              placeholder=\"Write your messages here...\"\n              required\n            ></textarea>\n          </div>\n        </div>\n        <div class=\"md:flex md:items-center\">\n          <div class=\"md:w-1/3\">\n            <button\n              type=\"submit\"\n              style=\"\n    padding: 10px 20px;\n    background: #2B6CB0;\n    color: white;\n    border-radius: 5px;\n\"\n            >Send</button>\n          </div>\n          <div class=\"md:w-2/3\"></div>\n        </div>\n      </form>\n    </div>\n  </Layout>\n</template>\n\n<script>\nimport Smile from \"../../static/smile.svg\";\nimport Below1 from \"../../static/corner-right-down.svg\";\nimport Below2 from \"../../static/corner-left-down.svg\";\n\nexport default {\n  metaInfo: {\n    title: \"Home\"\n  },\n  components: {\n    Smile,\n    Below1,\n    Below2\n  }\n};\n</script>\n\n"],"sourceRoot":"src/pages"}]}